{
    "compilerOptions": {
        "outDir": "./dist",
        "jsx": "react-jsx",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "components/*": [
                "src/components/*"
            ]
        },
        "types": [
            "node",
            "vite/client"
        ]
    },
    "include": [
        "src"
    ]
}
